<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.udacoding">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Udacoding"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".bottombav.BottomActivity"></activity>
        <activity android:name=".crud.CrudActivity" />
        <activity android:name=".database.view.splash.SplashActivity" />
        <activity android:name=".database.view.HomeActivity" />
        <activity android:name=".database.view.RegisterActivity" />
        <activity android:name=".database.view.LoginActivity" />
        <activity android:name=".recyclerview.view.DetailActivity" />
        <activity android:name=".recyclerview.view.RecyclerActivity" />
        <activity android:name=".kalkulator.view.KalkulatorActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>